{
  "name": "owncloud/files_external_s3",
  "description": "ownCloud primary storage integration using S3 protocol",
  "require": {
    "dg/composer-cleaner": "^2.0",
    "aws/aws-sdk-php": "^3.48",
    "guzzlehttp/guzzle": "^5.3.3",
    "typisttech/imposter-plugin": "^0.3.0"
  },
  "license": "GPL-2.0",
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": false,
    "platform": {
      "php": "7.0.8"
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.2"
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false
    },
    "imposter": {
      "namespace": "OCA\\Files_Primary_S3\\Vendor",
      "excludes": [
        "dg/composer-cleaner"
      ]
    }
  }
}
