{
  "name": "owncloud/files_primary_s3",
  "description": "ownCloud primary storage integration using S3 protocol",
  "require": {
    "owncloud/core-replace": "@dev",
    "dg/composer-cleaner": "^2.0",
    "aws/aws-sdk-php": "^3.48"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../build/core-replace",
      "options": { "symlink": false }
    }
  ],
  "license": "GPL-2.0",
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": false,
    "platform": {
      "php": "7.0.0"
    }
  },

  "require-dev": {
    "owncloud/coding-standard": "^1.0"
  }
}
