{
  "name": "owncloud/files_external_s3",
  "description": "ownCloud primary storage integration using S3 protocol",
  "require": {
    "dg/composer-cleaner": "^2.0",
    "aws/aws-sdk-php": "3.232.*"
  },
  "replace": {
    "guzzlehttp/guzzle": "*",
    "guzzlehttp/psr7": "*"
  },
  "license": "GPL-2.0",
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": false,
    "platform": {
      "php": "7.3"
    },
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true,
      "dg/composer-cleaner": true
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.4"
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false
    }
  }
}
