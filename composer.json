{
  "name": "owncloud/files_primary_s3",
  "description": "ownCloud primary storage integration using S3 protocol",
  "require": {
    "dg/composer-cleaner": "^2.0",
    "aws/aws-sdk-php": "3.283.*"
  },
  "replace": {
    "guzzlehttp/guzzle": "*",
    "guzzlehttp/promises": "*",
    "guzzlehttp/psr7": "*",
    "psr/http-message": "*"
  },
  "license": "GPL-2.0",
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": false,
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true,
      "dg/composer-cleaner": true
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false
    }
  }
}
